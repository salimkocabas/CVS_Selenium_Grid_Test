<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="regrression paralel" >
  <test name="Regression Test"  parallel="classes"   thread-count="2">
    <groups>
			<run>
				<include name="regression" />
			</run>
		</groups>
    <classes>
      <class name="com.test.BaseClass"/>
      <class name="com.test.CVS"/>
      <class name="com.test.TestB"/>
      <class name="com.test.TestA"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->

